{% extends('base.html.twig') %}

{% block body %}
<table style="width:100%">
    {% for challenge in challenges %}
    <tr>
        <td><a href="{{path('showChallenge', {uuid: challenge.uuid})}}">{{ challenge.title }}</a></td>
        <td><a href="{{path('editChallenge', {uuid: challenge.uuid})}}">Edit</a></td>
        <td><a href="{{path('deleteChallenge', {uuid: challenge.uuid})}}">Delete</a></td>
    </tr>
    {% endfor %}
</table>
{% endblock %}
